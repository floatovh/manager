{
  "name": "@ovh-ux/manager-product-listing-configuration",
  "version": "0.0.0",
  "private": true,
  "description": "Set of columns configuration for OVHcloud products",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/modules/models"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/product-configuration.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-product-listing-configuration' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-product-listing-configuration' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-product-listing-configuration' --include-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "@ovh-ux/manager-config": "^0.4.0",
    "i18next": "^19.6.2",
    "i18next-http-backend": "^1.0.17"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.2.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-multi-entry": "^3.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "glob": "^7.1.6"
  }
}
